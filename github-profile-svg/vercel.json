{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/profile",
            "destination": "/api/profile.js"
        }
    ],
    "headers": [
        {
            "source": "/api/profile",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "image/svg+xml"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400, s-maxage=86400, stale-while-revalidate=3600"
                }
            ]
        }
    ]
}